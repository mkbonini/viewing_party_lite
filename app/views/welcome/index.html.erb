<% if !session[:user_id] %>
    <%= button_to("Create a New User", "/users/new", method: :get)  %>
    <%= button_to("Log In", "/login", method: :get)  %>
<% else %>
    <%= button_to("Log out", "/logout", method: :delete)  %>

    <div id='existing-users'>
    <p> Existing Users </p>
    <% @users.each do |user| %>
        <%= user.email %>
    <% end %>
</div>
<% end %>

